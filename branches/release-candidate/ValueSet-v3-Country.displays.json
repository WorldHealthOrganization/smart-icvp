            "^[a-z]{2}(-[A-Z]{2})?$": {
              "type": "string",
              "description": "Display value in the specified language (ISO 639-1 code)"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  },
